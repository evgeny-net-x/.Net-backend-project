@model Backend4.Models.SignUp.SignUpCredentialsViewModel
@{
	ViewBag.Title = "Sign up";

	String[] months = {
		"January",
		"February",
		"March",
		"April",
		"May",
		"June",
		"July",
		"August",
		"September",
		"October",
		"November",
		"December"
	};
}

<h2>Sign up result</h2>
<dl class="dl-horizontal">
    <dt>First name</dt>
    <dd>@Model.FirstName</dd>
    <dt>Last name</dt>
    <dd>@Model.LastName</dd>
    <dt>Birthday</dt>
    <dd>@months[Model.BirthdayMonth - 1] @Model.BirthdayDay, @Model.BirthdayYear.</dd>
    <dt>Email</dt>
    <dd>@Model.Email</dd>
    <dt>Password</dt>
    <dd>@Model.Password</dd>
    <dt>Gender</dt>
    <dd>@Model.Gender</dd>
    <dt>User existed</dt>
    <dd>@Model.Existed</dd>
    <dt>Remembered</dt>
    <dd>@Model.Remember</dd>
</dl>
